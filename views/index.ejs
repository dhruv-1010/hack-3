<%- include('./nav') %>

<div id="quotesBox">
    <% for (let item of quotesD) { %>
        <div class="box">
            <div>
                <h1><%= item.quote %></h1>
                <p><%= item.name %></p>
            </div>
        <a href="/show/<%=item._id%>">
        <button class="btn">
                Show Quote
            </button>
        </a>
        </div>
   <% } %>
</div>

</body>
</html>