<%- include('./nav') %>

<div id="quotesBox">
    <% for (let item of quotesD) { %>
        <div class="box">
            <div>
                <h1><%= item.quote %></h1>
                <p><%= item.name %></p>
            </div>
        <button class="btn"><a href="/show/<%=item._id%>">Show Quote</a></button>
        </div>
   <% } %>
</div>

</body>
</html>